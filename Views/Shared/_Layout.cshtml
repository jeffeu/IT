<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/coral.css")

</head>

<body>
    <div id="content">
        <div class="navbar navbar-primary main" style="visibility: visible;">
            <div class="col-md-3">
                <div class="input-group">
                    <p class="logo">Intellectus Training</p>
                </div>
            </div>

        </div>
        @Scripts.Render("~/coral.js")
        @Scripts.Render("~/mask.js")
        @RenderSection("scripts", required: false)
        <div class="container">
            @RenderBody()
            <footer>
                <p>&copy; @DateTime.Now.Year - Jefferson Rossi Florencio (Jefferson.florencio@live.com)</p>
            </footer>
        </div>


    </div>

    

</body>
</html>
